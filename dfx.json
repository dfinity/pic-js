{
  "canisters": {
    "counter": {
      "main": "examples/counter/src/main.mo",
      "type": "motoko",
      "optimize": "cycles",
      "declarations": {
        "bindings": ["js", "ts"],
        "output": "examples/counter/declarations"
      }
    },
    "clock": {
      "main": "examples/clock/src/main.mo",
      "type": "motoko",
      "optimize": "cycles",
      "declarations": {
        "bindings": ["js", "ts"],
        "output": "examples/clock/declarations"
      }
    },
    "todo": {
      "type": "rust",
      "candid": "examples/todo/todo.did",
      "optimize": "cycles",
      "package": "todo",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "examples/todo/declarations"
      }
    }
  },
  "output_env_file": ".env",
  "dfx": "0.15.2",
  "version": 1
}
